{
  "name": "compare-package-json-version-with-latest",
  "version": "1.0.0",
  "description": "Allows us to compare your local `package.json` version vs. the current registry latest version.",
  "main": "dist/index.js",
  "scripts": {
    "start": "$(yarn bin ncc) run src/run.ts",
    "build": "$(yarn bin ncc) build src/run.ts",
    "jest": "NODE_ENV=test $(yarn bin jest)",
    "jest:ci": "yarn jest --runInBand --ci --colors --collectCoverage",
    "jest:tdd": "yarn jest --watch --verbose"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.3.0",
    "@vercel/ncc": "^0.28.5",
    "latest-version": "^5.1.0",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/node": "^15.0.2",
    "@types/semver": "^7.3.5",
    "flush-promises": "^1.0.2",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  }
}
